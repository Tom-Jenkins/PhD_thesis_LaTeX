doi = {10.1111/mec.12263},
file = {:C$\backslash$:/Users/tj248/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ahrens et al. - 2013 - The curious case of Hermodice carunculata (Annelida Amphinomidae) Evidence for genetic homogeneity throughout the.pdf:pdf},
isbn = {1365-294X (Electronic)$\backslash$r0962-1083 (Linking)},
