file = {:C$\backslash$:/Users/tj248/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Dannewitz et al. - 2005 - Panmixia in the European eel a matter of time(2).pdf:pdf},
issn = {0962-8452},
journal = {Proceedings. Biological sciences / The Royal Society},
keywords = {Anguilla,Anguilla: genetics,Anguilla: physiology,Animals,Atlantic Ocean,Cluster Analysis,Demography,Europe,Fresh Water,Genetic Variation,Genetics, Population,Geography,Microsatellite Repeats,Microsatellite Repeats: genetics,Population Dynamics,Time Factors},
month = {jun},
number = {1568},
pages = {1129--37},
pmid = {16024374},
title = {{Panmixia in the European eel: a matter of time...}},
