file = {:C$\backslash$:/Users/tj248/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ali et al. - 2016 - Rad capture (Rapture) Flexible and efficient sequence-based genotyping.pdf:pdf},
isbn = {8013186628},
issn = {19432631},
journal = {Genetics},
keywords = {Genotyping,Massively parallel sequencing,Population genetics,Rainbow trout,Restriction-site associated DNA (RAD),Sequence capture},
number = {2},
