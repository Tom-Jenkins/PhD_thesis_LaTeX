issn = {0962-1083},
journal = {Molecular ecology},
keywords = {Animal Migration,Animals,Atlantic Ocean,Biological Evolution,Flounder,Flounder: classification,Flounder: genetics,Gene Flow,Genetic Variation,Microsatellite Repeats,Oceans and Seas,Population Density,Population Dynamics},
month = {aug},
