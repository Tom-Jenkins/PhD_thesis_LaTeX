issn = {0962-1083},
journal = {Molecular Ecology},
keywords = {16S,Americas,Animals,Bayes Theorem,Biodiversity,Decapoda (Crustacea),Decapoda (Crustacea): genetics,Gene Flow,Genetic Speciation,Genetic Variation,Genetics,Haplotypes,Haplotypes: genetics,Myosin Heavy Chains,Myosin Heavy Chains: genetics,Phylogeny,Population,RNA,Ribosomal},
month = {nov},
number = {13},
pages = {4049--4063},
pmid = {17054502},
title = {{Cryptic biodiversity and phylogeographical patterns in a snapping shrimp species complex.}},
