file = {:C$\backslash$:/Users/tj248/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Hewitt - 2004 - Genetic consequences of climatic oscillations in the Quaternary(4).pdf:pdf},
isbn = {09628436},
issn = {0962-8436},
journal = {Philosophical Transactions of the Royal Society B},
keywords = {Animals,Biodiversity,Biological Evolution,Climate,DNA,DNA: genetics,Demography,Environment,Geography,Plants,Plants: genetics,Population Dynamics,biodiversity,colonization,hybrid zones,ice ages,phylogeography,refugia},
month = {feb},
number = {1442},
