keywords = {Animals,Automatic Data Processing,Automatic Data Processing: methods,Automatic Data Processing: standards,DNA,Genetic Variation,Genetic Variation: genetics,Mitochondrial,Mitochondrial: genetics,Molecular Sequence Data,Phylogeny,Research Design,Sample Size},
month = {dec},
number = {12},
pages = {e422},
pmid = {16336051},
title = {{DNA barcoding: error rates based on comprehensive sampling.}},
