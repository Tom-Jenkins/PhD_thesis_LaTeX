pages = {e36921},
pmid = {22679482},
title = {{Human impact on atolls leads to coral loss and community homogenisation: a modeling study.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3367966{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {7},
