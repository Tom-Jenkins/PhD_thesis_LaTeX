file = {:C$\backslash$:/Users/tj248/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Souza et al. - 2017 - Efficiency of ddRAD target enriched sequencing across spiny rock lobster species (Palinuridae Jasus).pdf:pdf},
isbn = {4159801706},
issn = {2045-2322},
journal = {Scientific Reports},
number = {1},
pages = {6781},
publisher = {Springer US},
title = {{Efficiency of ddRAD target enriched sequencing across spiny rock lobster species (Palinuridae: Jasus)}},
url = {http://www.nature.com/articles/s41598-017-06582-5},
volume = {7},
year = {2017}
