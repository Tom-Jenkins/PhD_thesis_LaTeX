file = {:C$\backslash$:/Users/tj248/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Stamatis et al. - 2004 - Mitochondrial DNA variation in Northeast Atlantic and Mediterranean populations of Norway lobster, Nephrops (3).pdf:pdf},
isbn = {2410579310},
issn = {0962-1083},
journal = {Molecular ecology},
keywords = {Analysis of Variance,Animals,Atlantic Ocean,Cluster Analysis,DNA Primers,DNA, Mitochondrial,DNA, Mitochondrial: genetics,Demography,Genetic Variation,Genetics, Population,Geography,Haplotypes,Haplotypes: genetics,Nephropidae,Nephropidae: genetics,Phylogeny,Polymorphism, Restriction Fragment Length,Population Dynamics},
