journal = {Journal of Crustacean Biology},
keywords = {Geographic variation,Homarus americanus,Lobster larvae,Temperature,development rate},
number = {4},
pages = {527--536},
title = {{Effect of temperature on development rate of larvae from cold-water American lobster (\textit{Homarus americanus})}},
volume = {33},
year = {2013}
}


