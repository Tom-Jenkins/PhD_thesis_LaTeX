@article{Shi2011,
abstract = {{\textless}sec{\textgreater} {\textless}title{\textgreater}Background{\textless}/title{\textgreater} {\textless}p{\textgreater}The distribution of genetic diversity among plant populations growing along elevational gradients can be affected by neutral as well as selective processes. Molecular markers used to study these patterns usually target neutral processes only, but may also be affected by selection. In this study, the effects of elevation and successional stage on genetic diversity of a dominant tree species were investigated controlling for neutrality of the microsatellite loci used.{\textless}/p{\textgreater} {\textless}/sec{\textgreater} {\textless}sec{\textgreater} {\textless}title{\textgreater}Methodology/Principal Findings{\textless}/title{\textgreater} {\textless}p{\textgreater}Diversity and differentiation among 24 populations of {\textless}italic{\textgreater}Castanopsis eyrei{\textless}/italic{\textgreater} from different elevations (251â920 m) and successional stages were analysed by eight microsatellite loci. We found that one of the loci (Ccu97H18) strongly deviated from a neutral model of differentiation among populations due to either divergent selection or hitchhiking with an unknown selected locus. The analysis showed that {\textless}italic{\textgreater}C. eyrei{\textless}/italic{\textgreater} populations had a high level of genetic diversity within populations ({\textless}italic{\textgreater}A{\textless}sub{\textgreater}R{\textless}/sub{\textgreater}{\textless}/italic{\textgreater} = 7.6, {\textless}italic{\textgreater}H{\textless}sub{\textgreater}E{\textless}/sub{\textgreater}{\textless}/italic{\textgreater} = 0.82). Genetic variation increased with elevation for both the putatively selected locus Ccu97H18 and the neutral loci. At locus Ccu97H18 one allele was dominant at low elevations, which was replaced at higher elevations by an increasing number of other alleles. The level of genetic differentiation at neutral loci was similar to that of other Fagaceae species ({\textless}italic{\textgreater}F{\textless}sub{\textgreater}ST{\textless}/sub{\textgreater}{\textless}/italic{\textgreater} = 0.032, {\textless}inline-formula{\textgreater}{\textless}inline-graphic xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="info:doi/10.1371/journal.pone.0021302.e001" mimetype="image" xlink:type="simple"/{\textgreater}{\textless}/inline-formula{\textgreater} = 0.15). Population differentiation followed a model of isolation by distance but additionally, strongly significant isolation by elevation was found, both for neutral loci and the putatively selected locus.{\textless}/p{\textgreater} {\textless}/sec{\textgreater} {\textless}sec{\textgreater} {\textless}title{\textgreater}Conclusions/Significance{\textless}/title{\textgreater} {\textless}p{\textgreater}The results indicate higher gene flow among similar elevational levels than across different elevational levels and suggest a selective influence of elevation on the distribution of genetic diversity in {\textless}italic{\textgreater}C. eyrei{\textless}/italic{\textgreater}. The study underlines the importance to check the selective neutrality of marker loci in analyses of population structure.{\textless}/p{\textgreater} {\textless}/sec{\textgreater}},
author = {Shi, Miao Miao and Michalski, Stefan G. and Chen, Xiao Yong and Durka, Walter},
doi = {10.1371/journal.pone.0021302},
file = {:C$\backslash$:/Users/tj248/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Shi et al. - 2011 - Isolation by elevation Genetic structure at neutral and putatively non-neutral loci in a dominant tree of subtrop(2).pdf:pdf},
issn = {19326203},
