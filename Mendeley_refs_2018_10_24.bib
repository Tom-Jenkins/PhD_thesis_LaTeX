isbn = {1365-294X},
issn = {09621083},
journal = {Molecular Ecology},
keywords = {Corallium rubrum,dispersal,drift,heterozygote deficiency,kinship analysis,microsatellites,photogrammetry,spatial genetic structure},
number = {19},
