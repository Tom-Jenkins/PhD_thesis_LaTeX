journal = {Heredity},
keywords = {bottlenecks,demographic inference,linkage disequilibrium,out-of-africa,site-frequency spectrum,snps},
number = {5},
pages = {409--419},
pmid = {23423148},
publisher = {Nature Publishing Group},
title = {{Inferring population size changes with sequence and SNP data: lessons from human bottlenecks}},
