issn = {1932-6203},
journal = {PLoS ONE},
keywords = {Animals,Chromosome Mapping,Chromosome Mapping: methods,Expressed Sequence Tags,Genetic Markers,Genome,Genotype,Methods,Neurospora crassa,Neurospora crassa: genetics,Polymorphism,Restriction Mapping,Single Nucleotide,Smegmamorpha,Smegmamorpha: genetics},
month = {jan},
