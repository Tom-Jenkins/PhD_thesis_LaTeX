journal = {Marine Ecology Progress Series},
keywords = {Genetic variation,Microsatellites,Population structure,Sweepstakes reproductive success,Western rock lobster},
pages = {213--221},
title = {{Spatial scales of genetic patchiness in the western rock lobster Panulirus cygnus}},
volume = {486},
year = {2013}
}
