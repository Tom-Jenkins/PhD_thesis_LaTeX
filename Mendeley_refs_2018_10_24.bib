journal = {Environmental Biology of Fishes},
keywords = {Arctic,Coast,European lobster,Homarus,\textit{Homarus gammarus},Lobster,Norway,Samples,Size,System,abundance,alleles,allozyme,assessment,characteristics,coastal,condition,conditions,differences,dispersal,diversity,gammarus,gene,heterozygosity,larval,larval dispersal,local,mtdna,northwest,population,populations,reduction,sampling,south,support,systems,western},
