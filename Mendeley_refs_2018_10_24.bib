issn = {22421300},
journal = {BioInvasions Records},
keywords = {European lobster,\textit{Homarus gammarus},disease transfer,european lobster,\textit{Homarus gammarus},invasive,non-native,threat},
number = {1},
pages = {17--23},
title = {{Reports of American lobsters, Homarus americanus (H. Milne Edwards, 1837), in British waters}},
