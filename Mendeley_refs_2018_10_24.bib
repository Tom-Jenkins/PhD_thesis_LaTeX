@article{Haig2016,
abstract = {The field of conservation genetics, when properly implemented, is a constant juggling act integrating molecular genetics, ecology, and demography with applied aspects concerning managing declining species or implementing conser- vation laws and policies. This young field has grown substantially since the 1980s following the development of polymerase chain reaction and now into the genomics era. Our laboratory has âgrown up' with the field, having worked on these issues for over three decades. Our multidisciplinary approach entails under- standing the behavior and ecology of species as well as the underlying processes that contribute to genetic viability. Taking this holistic approach provides a com- prehensive understanding of factors that influence species persistence and evolu- tionary potential while considering annual challenges that occur throughout their life cycle. As a federal laboratory, we are often addressing the needs of the U.S. Fish and Wildlife Service in their efforts to list, de-list, or recover species. Never- theless, there remains an overall communication gap between research geneticists and biologists who are charged with implementing their results. Therefore, we outline the need for a National Center for Small Population Biology to ameliorate this problem and provide organizations charged with making status decisions firmer ground from which to make their critical decisions.},
author = {Haig, Susan M. and Miller, Mark P. and Bellinger, Renee and Draheim, Hope M. and Mercer, Dacey M. and Mullins, Thomas D.},
doi = {10.1111/eva.12337},
file = {:C$\backslash$:/Users/tj248/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Haig et al. - 2016 - The conservation genetics juggling act Integrating genetics and ecology, science and policy.pdf:pdf},
isbn = {1541750098},
issn = {17524571},
journal = {Evolutionary Applications},
keywords = {Annual cycle,Conservation genetics,Cross-seasonal interactions,Effective population size,Endangered species,Inbreeding,Migratory connectivity,Pedigree analyses},
number = {1},
pages = {181--195},
pmid = {27087847},
title = {{The conservation genetics juggling act: Integrating genetics and ecology, science and policy}},
volume = {9},
year = {2016}
}
@article{Laikre2010,
abstract = {Large-scale exploitation of wild animals and plants through fishing, hunting and logging often depends on augmentation through releases of translocated or captively raised individuals. Such releases are performed worldwide in vast numbers. Augmentation can be demographically and economically beneficial but can also cause four types of adverse genetic change to wild populations: (1) loss of genetic variation, (2) loss of adaptations, (3) change of population composition, and (4) change of population structure. While adverse genetic impacts are recognized and documented in fisheries, little effort is devoted to actually monitoring them. In forestry and wildlife management, genetic risks associated with releases are largely neglected. We outline key features of programs to effectively monitor consequences of such releases on natural populations. {\textcopyright} 2010 Elsevier Ltd.},
