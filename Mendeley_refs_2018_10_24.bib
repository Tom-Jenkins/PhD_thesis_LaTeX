journal = {Molecular ecology},
keywords = {Animals,Demography,Evolution, Molecular,Fishes,Fishes: genetics,Gene Frequency,Genetic Variation,Genotype,Isoenzymes,Linkage Disequilibrium,Microsatellite Repeats,Microsatellite Repeats: genetics,North Sea,Selection, Genetic},
