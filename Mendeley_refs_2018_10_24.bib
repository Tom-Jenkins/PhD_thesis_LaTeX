file = {:C$\backslash$:/Users/tj248/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lambeck et al. - 2014 - Sea level and global ice volumes from the Last Glacial Maximum to the Holocene.pdf:pdf},
isbn = {0027-8424},
