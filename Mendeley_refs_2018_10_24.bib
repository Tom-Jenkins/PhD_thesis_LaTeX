file = {:C$\backslash$:/Users/tj248/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Dupont et al. - 2009 - Fine- and regional-scale genetic structure of the exotic ascidian Styela clava (Tunicata) in southwest England(3).pdf:pdf},
issn = {1365-294X},
journal = {Molecular Ecology},
keywords = {Animals,Ecosystem,England,Gene Flow,Genetic Variation,Genotype,Humans,Larva,Larva: physiology,Marine Biology,Microsatellite Repeats,Population Dynamics,Urochordata,Urochordata: classification,Urochordata: genetics,Urochordata: growth {\&} development,Urochordata: physiology},
month = {feb},
number = {3},
pages = {442--453},
