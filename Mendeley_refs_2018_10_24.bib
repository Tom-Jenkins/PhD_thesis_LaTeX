issn = {1755-0998},
journal = {Molecular Ecology Resources},
keywords = {2013,accepted 18 september 2013,dna barcoding,insects,latitudinal biodiversity gradient,lepidoptera,received 2 august 2013,revision received 15 september,species interactions,systematics},
month = {mar},
number = {2},
