issn = {1553-7358},
journal = {PLoS Computational Biology},
month = {apr},
number = {4},
pages = {e1003537},
pmid = {24722319},
title = {{BEAST 2: a software platform for Bayesian evolutionary analysis.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3985171{\&}tool=pmcentrez{\&}rendertype=abstract},
