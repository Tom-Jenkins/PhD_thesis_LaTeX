journal = {Molecular Ecology Resources},
keywords = {FST,Genotyping by sequencing,genome scan,genome-environment association,local adaptation,outlier analysis},
pages = {n/a--n/a},
pmid = {27618670},
title = {{Breaking RAD: An evaluation of the utility of restriction site associated DNA sequencing for genome scans of adaptation}},
url = {http://dx.doi.org/10.1111/1755-0998.12596},
year = {2016}
}
@article{Galla2016,
