pmid = {22164307},
title = {{Reconciling deep calibration and demographic history: bayesian inference of post glacial colonization patterns in Carcinus aestuarii (Nardo, 1847) and C. maenas (Linnaeus, 1758).}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3229605{\&}tool=pmcentrez{\&}rendertype=abstract},
