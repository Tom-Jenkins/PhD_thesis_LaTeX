keywords = {Depth distribution,Glaciations,MtDNA,Oceanographic discontinuities,Population structure},
pages = {664--672},
pmid = {22138160},
title = {{Phylogeographic patterns of decapod crustaceans at the Atlantic-Mediterranean transition}},
