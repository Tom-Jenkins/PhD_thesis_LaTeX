author = {Zlojutro, Mark and Rubicz, Rohina and Devor, Eric J. and Spitsyn, Victor A. and Makarov, Sergei V. and Wilson, Kristin and Crawford, Michael H.},
doi = {10.1002/ajpa.20287},
file = {:C$\backslash$:/Users/tj248/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Zlojutro et al. - 2006 - Genetic structure of the aleuts and circumpolar populations based on mitochondrial DNA sequences A synthesis.pdf:pdf},
isbn = {0002-9483 (Print)},
