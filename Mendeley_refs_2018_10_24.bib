author = {Yang, Sung-Yin and Keshavmurthy, Shashank and Obura, David and Sheppard, Charles R C and Visram, Shakil and Chen, Chaolun Allen},
doi = {10.1371/journal.pone.0035836},
file = {:C$\backslash$:/Users/tj248/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Yang et al. - 2012 - Diversity and distribution of Symbiodinium associated with seven common coral species in the Chagos Archipelago,(3).pdf:pdf},
