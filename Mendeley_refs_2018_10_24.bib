isbn = {1877-7260},
issn = {1877-7252},
journal = {Conservation Genetics Resources},
keywords = {Genetic erosion,Hybridisation,Inbreeding,Wildlife,,africa,genetic erosion,hybridisation,inbreeding,selective breeding,small populations,southern,translocation,wildlife},
number = {0},
pages = {0},
publisher = {Springer Netherlands},
title = {{âIntentional Genetic Manipulation' as a conservation threat}},
