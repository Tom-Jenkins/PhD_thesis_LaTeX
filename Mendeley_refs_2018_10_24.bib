@article{Mirams2011,
abstract = {Biogeographic barriers, some transitory in duration, are likely to have been important contributing factors to modern marine biodiversity in the Indo-Pacific region. One such barrier was the Torres Strait land bridge between continental Australia and New Guinea that per- sisted through much of the late Pleistocene and separated Indian and Pacific Ocean taxa. Here, we examine the pat- terns of mitochondrial DNA diversity for marine animals with present-day distributions spanning the Torres Strait. Specifically, we investigate whether there are concordant signatures across species, consistent with either vicariance or recent colonization from either ocean basin. We survey four species of reef fishes (Apogon doederleini, Poma- centrus coelestis, Dascyllus trimaculatus, and Acanthurus triostegus) for mtDNA cytochrome oxidase 1 and control region variation and contrast these results to previous mtDNA studies in diverse marine animals with similar distributions. We find substantial genetic partitioning (estimated from F-statistics and coalescent approaches) between Indian and Pacific Ocean populations for many species, consistent with regional persistence through the late Pleistocene in both ocean basins. The species-specific estimates of genetic divergence, however, vary greatly and for reef fishes we estimate substantially different Communicated by Biology Editor Dr. Ruth Gates Electronic supplementary material The online version of this article (doi:10.1007/s00338-011-0767-x) contains supplementary material, which is available to authorized users. A. G. K. Mirams ? E. A. Treml ? J. L. Shields ? L. Liggins ? C. Riginos ({\&}) School of Biological Sciences, The University of Queensland, St. Lucia, QLD 4072, Australia e-mail: c.riginos@uq.edu.au divergence divergence times among species. It is likely that Indian and Pacific Ocean populations have been isolated for multiple glacial cycles for some species, whereas for other species genetic connections have been more recent. Regional estimates of genetic diversity and directionality of gene flow also vary among species. Thus, there is no apparent consistency among historical patterns across the Torres Strait for these co-distributed marine animals.},
annote = {A meta-analysis reviewing patterns of mitochondrial (COI {\&} CR) diversity of extant marine animals spanning the Torres Strait. A land bridge persisted through much of the late Pleistocene, which presumably acted as a geographical barrier separating Indian and Pacific Ocean taxa.},
author = {Mirams, A. G K and Treml, E. A. and Shields, J. L. and Liggins, L. and Riginos, C.},
doi = {10.1007/s00338-011-0767-x},
file = {:C$\backslash$:/Users/tj248/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Mirams et al. - 2011 - Vicariance and dispersal across an intermittent barrier Population genetic structure of marine animals across (2).pdf:pdf},
isbn = {doi:10.1007/s00338-011-0767-x},
issn = {07224028},
journal = {Coral Reefs},
keywords = {Australia,Coalescence,Comparative phylogeography,Coral Triangle,Gene flow,Planktonic larval dispersal},
pages = {937--949},
title = {{Vicariance and dispersal across an intermittent barrier: Population genetic structure of marine animals across the Torres Strait land bridge}},
