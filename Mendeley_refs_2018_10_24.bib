file = {:C$\backslash$:/Users/tj248/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Cowen et al. - 2000 - Connectivity of marine populations open or closed(2).pdf:pdf},
isbn = {0036-8075 (Print)},
issn = {00368075},
