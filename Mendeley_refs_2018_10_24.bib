file = {:C$\backslash$:/Users/tj248/OneDrive - University of Exeter/Mendeley/Ecological Applications 2003 Palumbi.pdf:pdf},
isbn = {1051-0761},
issn = {10510761},
journal = {Ecological Applications},
keywords = {Connectivity,Dispersal,Isolation by distance,Larvae,Population structure},
number = {1},
pages = {146--158},
pmid = {1725},
primaryClass = {10.1890},
title = {{Population genetics, demographic connectivity, and the design of marine reserves}},
volume = {13},
year = {2003}
}
@article{Schmalenbach2010,
abstract = {A laboratory-based study was performed to assess the impact of climate warming on the recruitment of the endangered population of the European lobster (\textit{Homarus gammarus}) at Helgoland (North Sea, German Bight). Egg-bearing females collected in situ just after spawning in late summer were subjected to various seasonal temperature regimes. Regimes with elevated temperatures (mild winters) resulted in a strong seasonal forward shift of larval hatching. Hatching took place at significantly lower temperatures than under regimes with normal winters. Experiments on larval development across a range of constant temperatures showed that no successful larval development occurred at temperatures below 14Â°C. Larval survival increased from 9{\%} at 14Â°C to 80{\%} at 22Â°C, while duration of larval development decreased correspondingly from 26 to 13 days. We hypothesize that an ongoing warming of the North Sea will strongly affect the recruitment success of the Helgoland lobster, mainly resulting from a decoupling of the seasonal peak appearance of larvae from optimal external conditions (temperature, food availability) for larval development.},
